class Solution:
    def get_min_max(self, arr):
        min = arr[0] 
        max = arr[0]
        for i in range(1,len(arr)):
            if min > arr[i] :
                min = arr[i]
            
        for i in range(1,len(arr)):
            if max < arr[i]:
                max = arr[i] 
                
        return(min , max )
            
            
